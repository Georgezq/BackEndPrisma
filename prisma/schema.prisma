generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model usuarios {
  id_user  Int    @id @default(autoincrement())
  nombre   String @unique @db.VarChar(100)
  password String @db.VarChar(100)
  rol      String @db.Char(2)
}

